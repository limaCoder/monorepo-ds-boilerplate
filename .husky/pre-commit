npx --no-install lint-staged &&

echo "\033[1;33mInit types checks into packages...\033[0m" &&

pnpm --filter @monorepo-ds-boilerplate/react --filter @monorepo-ds-boilerplate/tokens --filter !packages/eslint-config --filter !packages/ts-config exec tsc --noEmit --skipLibCheck &&

echo "\033[1;33mchecagem de tipos finalizada!\033[0m"